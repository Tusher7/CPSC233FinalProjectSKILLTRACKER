<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PrimeController">
   <children>
      <Label text="Language Learning">
         <font>
            <Font name="Candara Italic" size="36.0" />
         </font>
         <VBox.margin>
            <Insets left="400.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="47.0" prefWidth="600.0">
         <children>
            <Label text="What Languages would you like to learn?">
               <font>
                  <Font name="Book Antiqua" size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <CheckBox fx:id="frenchBox" mnemonicParsing="false" onAction="#LanguageSkillPickLogic" text="French">
         <font>
            <Font size="20.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="englishBox" mnemonicParsing="false" onAction="#LanguageSkillPickLogic" text="English">
         <font>
            <Font size="20.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="arabicBox" mnemonicParsing="false" onAction="#LanguageSkillPickLogic" text="Arabic">
         <font>
            <Font size="20.0" />
         </font>
      </CheckBox>
      <HBox prefHeight="39.0" prefWidth="600.0">
         <children>
            <Button fx:id="LanguagePickerBackButton" mnemonicParsing="false" onAction="#pickerViewBackButton" text="&lt; Back">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" top="10.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#secondSceneNextButton" text="Next &gt;">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin></Button>
            <Button fx:id="languagePickReset" mnemonicParsing="false" onAction="#languagePickResetButton" text="Reset">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="194.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="190.0" prefWidth="607.0">
               <children>
                  <Label fx:id="pickLanguage">
                     <font>
                        <Font name="Berlin Sans FB Demi Bold" size="25.0" />
                     </font>
                  </Label>
                  <Label fx:id="languageLabelOne" text="You need to spend roughly 1000 hours">
                     <font>
                        <Font name="Book Antiqua" size="22.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="languageLabelTwo" text="on each language to achieve mastery.">
                     <font>
                        <Font name="Book Antiqua" size="22.0" />
                     </font>
                  </Label>
                  <Label fx:id="languageLabelThree" textFill="#dd0f0f">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
